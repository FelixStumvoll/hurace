<UserControl x:Class="Hurace.RaceControl.Controls.RaceDisplayControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Hurace.RaceControl.Controls"
             xmlns:vm="clr-namespace:Hurace.RaceControl.ViewModels"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance vm:RaceDisplayViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
            <Button Content="Aktueller Rennläufer" Command="{Binding OpenCurrentSkierWindowCommand}"/>
    </Grid>
</UserControl>
