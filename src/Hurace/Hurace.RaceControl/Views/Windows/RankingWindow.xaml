<Window x:Class="Hurace.RaceControl.Windows.RankingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:s="clr-namespace:Hurace.RaceControl.Controls.SubViews"
        xmlns:local="clr-namespace:Hurace.RaceControl.Windows"
        mc:Ignorable="d"
        Title="RankingWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0" Text="Aktueller Zwischenstand:" FontSize="25" />
        <s:RankingView Grid.Row="1" />
    </Grid>
</Window>